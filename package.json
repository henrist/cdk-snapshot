{
  "name": "@henrist/cdk-snapshot",
  "version": "0.0.0-development",
  "description": "Create a snapshot of an CDK application",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-snapshot"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release",
    "snapshots": "./test-assemblies/build-all.sh"
  },
  "bin": {
    "cdk-snapshot": "lib/bin/cdk-snapshot.js"
  },
  "keywords": [
    "cdk",
    "snapshots"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.31.1",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "aws-cdk": "2.31.1",
    "aws-cdk-lib": "2.31.1",
    "aws-sdk": "2.1171.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "execa": "5.1.1",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "jest-cdk-snapshot": "2.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.3",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "cpy": "^8.1.0",
    "del": "^6.0.0",
    "glob": "^7.2.0"
  }
}
