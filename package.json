{
  "name": "@henrist/cdk-snapshot",
  "version": "0.0.0-development",
  "description": "Create a snapshot of an CDK application",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-snapshot"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release",
    "snapshots": "./test-assemblies/build-all.sh"
  },
  "bin": {
    "cdk-snapshot": "lib/bin/cdk-snapshot.js"
  },
  "keywords": [
    "cdk",
    "snapshots"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.108.1",
    "@aws-cdk/aws-lambda": "1.108.1",
    "@aws-cdk/core": "1.108.1",
    "@aws-cdk/custom-resources": "1.108.1",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.27",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "aws-cdk": "1.108.1",
    "aws-sdk": "2.918.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-prettier": "3.4.1",
    "execa": "5.1.1",
    "husky": "6.0.0",
    "jest": "27.1.1",
    "jest-cdk-snapshot": "1.4.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-jest": "27.0.5",
    "ts-node": "10.3.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "cpy": "^8.1.0",
    "del": "^6.0.0",
    "glob": "7.1.7"
  }
}
