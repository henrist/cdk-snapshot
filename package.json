{
  "name": "@henrist/cdk-snapshot",
  "version": "0.0.0-development",
  "description": "Create a snapshot of an CDK application",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-snapshot"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "snapshots": "./test-assemblies/build-all.sh"
  },
  "bin": {
    "cdk-snapshot": "lib/bin/cdk-snapshot.js"
  },
  "keywords": [
    "cdk",
    "snapshots"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.73.0",
    "@aws-cdk/aws-lambda": "1.73.0",
    "@aws-cdk/core": "1.73.0",
    "@aws-cdk/custom-resources": "1.73.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/rimraf": "3.0.0",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "aws-cdk": "1.73.0",
    "aws-sdk": "2.715.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "execa": "4.1.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-cdk-snapshot": "1.4.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.2.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "cpy": "^8.1.0",
    "del": "^6.0.0",
    "glob": "7.1.6"
  }
}
