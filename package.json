{
  "name": "@henrist/cdk-snapshot",
  "version": "0.0.0-development",
  "description": "Create a snapshot of an CDK application",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-snapshot"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release",
    "snapshots": "./test-assemblies/build-all.sh"
  },
  "bin": {
    "cdk-snapshot": "lib/bin/cdk-snapshot.js"
  },
  "keywords": [
    "cdk",
    "snapshots"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.1.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "aws-cdk": "2.1.0",
    "aws-cdk-lib": "2.1.0",
    "aws-sdk": "2.1045.0",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "execa": "5.1.1",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-cdk-snapshot": "2.0.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "cpy": "^8.1.0",
    "del": "^6.0.0",
    "glob": "^7.2.0"
  }
}
