{
  "name": "@henrist/cdk-snapshot",
  "version": "0.0.0-development",
  "description": "Create a snapshot of an CDK application",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-snapshot"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release",
    "snapshots": "./test-assemblies/build-all.sh"
  },
  "bin": {
    "cdk-snapshot": "lib/bin/cdk-snapshot.js"
  },
  "keywords": [
    "cdk",
    "snapshots"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.31.1",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@types/jest": "28.1.8",
    "@types/node": "18.15.11",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "aws-cdk": "2.31.1",
    "aws-cdk-lib": "2.31.1",
    "aws-sdk": "2.1171.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "execa": "5.1.1",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "jest-cdk-snapshot": "2.0.1",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "cpy": "^8.1.0",
    "del": "^6.0.0",
    "glob": "^7.2.0"
  }
}
